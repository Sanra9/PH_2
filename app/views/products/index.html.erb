<div class = "container-fluid p-5 bg-primary text-white text-center">
    <h1>My First Booststrap Page</h1>
    <p>Resize this responsive page to see the effect!</p>
</div>

<div class = "container"
    <div class = "row">
        <div class = "col-ms-12">

            <h2>Productos</h2>
            <div class="text-end mb-3">
                <%= link_to "Publicar Producto", new_product_path, class: "btn btn-outline-primary" %>
            </div>
            <table class = "table">
            <tr class = "table-dark color: white">
                <th>Id</th>
                <th>Nombre</th>
                <th>URL</th>
                <th>Descripci√≥n</th>
                <th></th>
            </tr>
            <% @products.each do |product| %>
                <tr>
                <td><%= product.id %></td>
                <td><%= link_to product.name, product, class: "btn btn-sm btn-outline-info" %></td>
                <td><%= product.url %></td>
                <td><%= product.description %></td>
                <td><%= link_to "Editar", edit_product_path(product), class: "btn btn-outline-warning" %></td>
                </tr>
            <% end %>
            </table>
        </div>
    </div>
</div>

